<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.UserSettingsController">
   <children>
      <TextField fx:id="usernameField" layoutX="63.0" layoutY="106.0" editable="true" promptText="New Username"/>
      <Button layoutX="268.0" layoutY="106.0" mnemonicParsing="false" onAction="#changeUsername" text="Change username" />
      <Button layoutX="268.0" layoutY="185.0" mnemonicParsing="false" onAction="#changePassword" text="Change Password" />
      <PasswordField fx:id="passwordField" layoutX="63.0" layoutY="185.0" promptText="New Password"/>
      <Label fx:id="infoLabel" layoutX="63.0" layoutY="251.0" />
      <Button layoutX="63.0" layoutY="292.0" mnemonicParsing="false" onAction="#backToLobby" text="Back to menu" />
   </children>
</AnchorPane>
